{
	"name": "TransformCSV",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Dataflow",
						"type": "DatasetReference"
					},
					"name": "Sourcecsv"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dataflow_sink",
						"type": "DatasetReference"
					},
					"name": "dataflow_sink"
				}
			],
			"transformations": [
				{
					"name": "select"
				},
				{
					"name": "aggregate"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Name as string,",
				"          NOC as string,",
				"          Discipline as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Sourcecsv",
				"Sourcecsv select(mapColumn(",
				"          Name,",
				"          NOC,",
				"          Discipline",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select",
				"select aggregate(groupBy(Discipline),",
				"     NOC = count(NOC)) ~> aggregate",
				"aggregate filter(Discipline == 'Basketball') ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> dataflow_sink"
			]
		}
	}
}